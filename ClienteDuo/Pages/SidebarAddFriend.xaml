<UserControl x:Class="ClienteDuo.Pages.SidebarAddFriend"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ClienteDuo.Pages"
             xmlns:p="clr-namespace:ClienteDuo.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="250">
    <Grid Background="{DynamicResource BtnBackground}">
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="200" Height="100">
            <Label Content="***IDUSUARIO***"/>
            <TextBox TextWrapping="Wrap" Width="120"/>
            <Button Content="***ENVIAR***"/>

        </StackPanel>
        <Button Content="***CERRAR***" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="BtnClose"/>
    </Grid>
</UserControl>
